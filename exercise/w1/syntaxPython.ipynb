   "source": [
    "# Bài 1: Khởi tạo biến và tính toán đơn giản\n",
    "a = int(input(\"Nhập số a: \"))\n",
    "b = int(input(\"Nhập số b: \"))\n",
    "tong = a + b\n",
    "hieu = a - b\n",
    "tich = a * b\n",
    "thuong = a / b\n",
    "print(\"Tổng:\", tong)\n",
    "print(\"Hiệu:\", hieu)\n",
    "print(\"Tích:\", tich)\n",
    "print(\"Thương:\", thuong)\n"
   ]
